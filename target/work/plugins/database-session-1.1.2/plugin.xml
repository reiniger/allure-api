<plugin name='database-session' version='1.1.2' grailsVersion='1.3.3 &gt; *'>
  <author>Burt Beckwith</author>
  <authorEmail>beckwithb@vmware.com</authorEmail>
  <title>Database Session Plugin</title>
  <description>Stores HTTP sessions in a database</description>
  <documentation>http://grails.org/plugin/database-session</documentation>
  <type>DatabaseSessionGrailsPlugin</type>
  <resources>
    <resource>grails.plugin.databasesession.PersistentSession</resource>
    <resource>grails.plugin.databasesession.PersistentSessionAttribute</resource>
    <resource>grails.plugin.databasesession.PersistentSessionAttributeValue</resource>
    <resource>grails.plugin.databasesession.DatabaseCleanupJob</resource>
    <resource>grails.plugin.databasesession.DatabaseCleanupService</resource>
    <resource>grails.plugin.databasesession.GormPersisterService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>